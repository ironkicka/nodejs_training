<% if (val != null) { %>
<tr>
    <th style = "width: 25%;">
    <a class = "home" href="/home/<%= val.user_id %>/1"><%= val.user_id %>
    </a></th>
    <td style = "width:auto;"><%= val.message %> </td>
    <% 
    var d = new Date(val.created_at);
    var dstr = d.getFullYear() + '-' + (d.getMonth() + 1) + '-' + d.getDate() + ' ' + d.getHours() + ':' + d.getMinutes() + d.getSeconds();
    %>
    <td style = "width: 25%;"><%= dstr %> </td> 
</tr>
<% } %>